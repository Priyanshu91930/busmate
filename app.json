{
  "expo": {
    "name": "busmate",
    "slug": "busmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpeg",
    "scheme": "busmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Busmate needs your location to share the bus's live position with students during a trip.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Busmate needs your location to share the bus's live position with students during a trip.",
        "NSLocationAlwaysUsageDescription": "Busmate needs your location to share the bus's live position with students during a trip."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.bus_tracker",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleSignIn": {
          "certificateHash": "5e8f16062ea3cd2c4a0d547876baa6f38cabf625"
        },
        "googleMaps": {
          "apiKey": "AIzaSyBE39MT5CfESMYrOCPutmZ5gL3Yf71dT7w"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0c4a6e"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.742393683381-p1ruuvbbrhoq41hrb0bngbuv2bo6ef3q"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Busmate needs your location to share the bus's live position with students during a trip."
        }
      ],
      "expo-camera"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c69b95b7-fc10-45a1-8c90-be2f28d4387a"
      }
    }
  }
}